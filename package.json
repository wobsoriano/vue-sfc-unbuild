  "typesVersions": {
    "*": {
      "*": [
        "*",
        "dist/*",
        "dist/*.d.ts",
        "dist/*/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "vite serve dev",
    "dev:build": "vite build dev",
    "dev:preview": "vite preview dev",
    "lint": "eslint .",
    "prepublishOnly": "pnpm build"
  },
  "peerDependencies": {
    "vue": "^3.2.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.37.0",
    "@types/node": "^18.15.10",
    "@vitejs/plugin-vue": "^4.1.0",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.36.0",
    "typescript": "^5.0.2",
    "unbuild": "^1.1.2",
    "vite": "^4.2.1",
    "vue": "^3.2.47"
  }
